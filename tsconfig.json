{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@/i18n": [
                "./src/i18n/index.ts"
            ],
            "@/components/*": [
                "./src/components/*"
            ]
        }
    }
}